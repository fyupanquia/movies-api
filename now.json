{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "DB_USER": "@pvideos-db-user",
    "DB_PASSWORD": "@pvideos-db-password",
    "DB_HOST": "@pvideos-db-host",
    "DB_NAME": "@pvideos-db-name"
  }
}